{
  "name": "mansa-marbles",
  "version": "0.1.0",
  "description": "A 3D web game based on Oware Nam Nam with educational elements about Mansa Musa",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "client": "vite client",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "vite build client --outDir ../dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/META4-Designs/mansa-marbles.git"
  },
  "keywords": [
    "oware",
    "game",
    "three.js",
    "mancala",
    "mansa-musa",
    "educational"
  ],
  "author": "META4-Designs",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "bcrypt": "^5.1.0",
    "cannon-es": "^0.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "gsap": "^3.12.7",
    "helmet": "^6.1.5",
    "howler": "^2.2.4",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "stats.js": "^0.17.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^2.8.7",
    "three": "^0.176.0",
    "three-csg-ts": "^3.2.0",
